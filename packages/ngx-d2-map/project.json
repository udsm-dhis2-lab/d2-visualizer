{
  "projectType": "library",
  "root": "packages/ngx-d2-map",
  "sourceRoot": "packages/ngx-d2-map/src",
  "prefix": "iapps",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ngx-d2-map",
        "main": "packages/ngx-d2-map/src/index.ts",
        "tsConfig": "packages/ngx-d2-map/tsconfig.lib.json",
        "assets": [
          "packages/ngx-d2-map/*.md",
          {
            "glob": "**/*",
            "input": "node_modules/monaco-editor",
            "output": "assets/monaco-editor"
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/ngx-d2-map"],
      "options": {
        "jestConfig": "packages/ngx-d2-map/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/ngx-d2-map/src/**/*.ts",
          "packages/ngx-d2-map/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
